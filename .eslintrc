{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1, { "extensions": [".js", ".jsx"] }
    ],
    "react/forbid-prop-types": [0, { "forbid": [] }],
    "react/prefer-stateless-function": "off",
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "quotes": [1, "double"]
  },
  "env": {
    "jest": true,
    "node": true
  }
}